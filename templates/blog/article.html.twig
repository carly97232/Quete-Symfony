{% extends 'base.html.twig' %}


{% block body %}

    <h1>Articles</h1>

    {% for article in articles %}
        <div>
            <a href="{{ path('article_show',{'id': article.id}) }}"><h2>{{ article.title }}</h2></a>
        </div>
    {% endfor %}
    <a href="{{ path('blog_category_index') }}">
        Back to Category.
    </a><br/>
    <a href="{{ path('blog_index') }}">
        Back to Blog.
    </a>

{% endblock %}
